let num1 = 3;
let num2 = 3;
let resultado;

console.log("Número Um: ",num1)
console.log("Número Dois: ",num2)

resultado = num1 + num2;
console.log("Soma: ", resultado);

resultado = num1 - num2;
console.log("Subtração: ", resultado);

resultado = num1 * num2;
console.log("Multiplicação: ", resultado);

resultado = num1 / num2;
console.log("Divisão: ", resultado);